local CoreGui = game:GetService('CoreGui')
local TweenService = game:GetService('TweenService')
local StarterGui = game:GetService('StarterGui')
local RunService = game:GetService('RunService')

pcall(function()
    StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
end)

local BypassScreen = Instance.new('ScreenGui')
BypassScreen.Name = 'BypassScreen'
BypassScreen.Parent = CoreGui
BypassScreen.IgnoreGuiInset = true
BypassScreen.DisplayOrder = 2147483647
BypassScreen.ZIndexBehavior = Enum.ZIndexBehavior.Global

local Background = Instance.new('Frame', BypassScreen)
Background.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
Background.Size = UDim2.new(1, 0, 1, 0)
Background.BackgroundTransparency = 0

local UIGradient = Instance.new('UIGradient', Background)
UIGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(40, 40, 55)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 10, 15)),
})
UIGradient.Rotation = 90

local MainFrame = Instance.new('Frame', Background)
MainFrame.Size = UDim2.new(0, 650, 0, 280)
MainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
MainFrame.BackgroundColor3 = Color3.fromRGB(20, 22, 28)
MainFrame.BackgroundTransparency = 0.1
local frameCorner = Instance.new('UICorner', MainFrame)
frameCorner.CornerRadius = UDim.new(0, 12)

local Stroke = Instance.new('UIStroke', MainFrame)
Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
Stroke.Thickness = 3
Stroke.LineJoinMode = Enum.LineJoinMode.Round

local StrokeGradient = Instance.new('UIGradient', Stroke)
StrokeGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 0, 0)),
    ColorSequenceKeypoint.new(0.16, Color3.fromRGB(255, 255, 0)),
    ColorSequenceKeypoint.new(0.33, Color3.fromRGB(0, 255, 0)),
    ColorSequenceKeypoint.new(0.50, Color3.fromRGB(0, 255, 255)),
    ColorSequenceKeypoint.new(0.66, Color3.fromRGB(0, 0, 255)),
    ColorSequenceKeypoint.new(0.83, Color3.fromRGB(255, 0, 255)),
    ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 0, 0)),
})

local Title = Instance.new('TextLabel', MainFrame)
Title.Text = 'PVB Anti Cheat Bypasser'
Title.Font = Enum.Font.Michroma
Title.TextSize = 30
Title.TextColor3 = Color3.fromRGB(220, 220, 255)
Title.BackgroundTransparency = 1
Title.Size = UDim2.new(1, -40, 0, 40)
Title.Position = UDim2.new(0.5, 0, 0, 30)
Title.AnchorPoint = Vector2.new(0.5, 0)

local StatusText = Instance.new('TextLabel', MainFrame)
StatusText.Text = 'working...'
StatusText.Font = Enum.Font.Code
StatusText.TextSize = 20
StatusText.TextColor3 = Color3.fromRGB(180, 180, 200)
StatusText.BackgroundTransparency = 1
StatusText.Size = UDim2.new(1, -40, 0, 30)
StatusText.Position = UDim2.new(0.5, 0, 0, 120)
StatusText.AnchorPoint = Vector2.new(0.5, 0)

local BarBackground = Instance.new('Frame', MainFrame)
BarBackground.BackgroundColor3 = Color3.fromRGB(10, 11, 14)
BarBackground.BorderSizePixel = 0
BarBackground.Size = UDim2.new(1, -60, 0, 12)
BarBackground.Position = UDim2.new(0.5, 0, 1, -80)
BarBackground.AnchorPoint = Vector2.new(0.5, 1)
local barBackCorner = Instance.new('UICorner', BarBackground)
barBackCorner.CornerRadius = UDim.new(1, 0)

local BarFill = Instance.new('Frame', BarBackground)
BarFill.BackgroundColor3 = Color3.fromRGB(0, 190, 255)
BarFill.BorderSizePixel = 0
BarFill.Size = UDim2.new(0, 0, 1, 0)
local barFillCorner = Instance.new('UICorner', BarFill)
barFillCorner.CornerRadius = UDim.new(1, 0)

local PercentageText = Instance.new('TextLabel', MainFrame)
PercentageText.Text = '0%'
PercentageText.TextColor3 = Color3.fromRGB(255, 255, 255)
PercentageText.BackgroundTransparency = 1
PercentageText.Size = UDim2.new(1, -60, 0, 30)
PercentageText.Position = UDim2.new(0.5, 0, 1, -50)
PercentageText.AnchorPoint = Vector2.new(0.5, 1)
PercentageText.Font = Enum.Font.Michroma
PercentageText.TextSize = 24

local function typewrite(object, text, duration)
    for i = 1, #text do
        object.Text = string.sub(text, 1, i)
        task.wait(duration / #text)
    end
end

spawn(function()
    while BypassScreen.Parent do
        StrokeGradient.Rotation = (tick() * 40) % 360
        task.wait()
    end
end)

task.wait(1)
typewrite(StatusText, 'Locating anti-cheat services...', 1.5)

local tweenInfo = TweenInfo.new(290, Enum.EasingStyle.Linear)
local goal = { Size = UDim2.new(1, 0, 1, 0) }
local tween = TweenService:Create(BarFill, tweenInfo, goal)
tween:Play()

local connection
connection = RunService.RenderStepped:Connect(function()
    local percentage = math.floor(BarFill.Size.X.Scale * 100)
    PercentageText.Text = tostring(percentage) .. '%'
end)

tween.Completed:Wait()
connection:Disconnect()
PercentageText.Text = '100%'
task.wait(1)

local bypassMessages = {
    'Bypassing game anti-cheat...',
    'Hooking into core functions...',
    'Disabling server-side checks...',
}

for _, message in ipairs(bypassMessages) do
    typewrite(StatusText, message, 1)
    task.wait(2)
end

local successColor = Color3.fromRGB(0, 255, 127)
TweenService:Create(Title, TweenInfo.new(0.5), { TextColor3 = successColor })
    :Play()
TweenService
    :Create(BarFill, TweenInfo.new(0.5), { BackgroundColor3 = successColor })
    :Play()
typewrite(Title, 'BYPASS COMPLETE', 0.5)
StatusText.Text = 'Waiting for game to load...'

task.wait(3)

local fadeOutTween = TweenService:Create(
    Background,
    TweenInfo.new(1.5),
    { BackgroundTransparency = 1 }
)
fadeOutTween:Play()
fadeOutTween.Completed:Wait()
BypassScreen:Destroy()

pcall(function()
    StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
end)
